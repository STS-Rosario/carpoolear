{
  "appId": "com.sts.carpoolear",
  "appName": "Carpoolear",
  "webDir": "dist/default/production/www",
  "server": {
    "androidScheme": "https"
  },
  "ios": {
    "minVersion": "14.0"
  },
  "plugins": {
    "StatusBar": {
      "style": "LIGHT",
      "backgroundColor": "#ffffff",
      "overlaysWebView": false
    },
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#ffffff",
      "showSpinner": false
    }
  }
}
