{
  "appId": "com.sts.carpoolear",
  "appName": "Carpoolear",
  "webDir": "www",
  "server": {
    "androidScheme": "https"
  },
  "ios": {
    "minVersion": "14.0"
  },
  "plugins": {
    "StatusBar": {
      "style": "LIGHT",
      "backgroundColor": "#ffffff",
      "overlaysWebView": false
    },
    "SplashScreen": {
      "launchShowDuration": 3000,
      "backgroundColor": "#ffffff",
      "showSpinner": false,
      "androidSplashResourceName": "splash",
      "splashFullScreen": true,
      "splashImmersive": true
    }
  }
}
